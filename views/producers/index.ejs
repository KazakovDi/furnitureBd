        <td>Имя</td>
        <td>Должность</td>
        <td>Отдел</td>
        <td>Паспорт</td>
    </tr>
    <% employees.forEach(employee => { %>
        <tr> 
            <td><%= employee.employeeName %></td>
            <td><%= employee.position%></td>
