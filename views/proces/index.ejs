
    <h1>Процессы</h1>
<table align="center"rules="cols">
    <tr>
        <td>Дата</td>
        <td>Детали</td>
        <td>Операция</td>
        <td>Изделие</td>
        <td>Мастер</td>
        <td>Отдел</td>
        <td>Количество</td>
        <td>Стоимость</td>
    </tr>
    <% procs.forEach(proc => { %>
        <tr> 
            <td><%= proc.processDate.toLocaleDateString() %></td>
            <td><%= proc.detail.productName%></td>
            <td><%= proc.processName %></td>
            <td><%= proc.product.productName%></td>
            <td><%= proc.master.employeeName%></td> 
            <%if(proc.departament !== undefined) { %>
                <td><%= proc.departament.departamentName%></td> 
            <%} else {%>
                <td></td>
                <%  }%>
            <td><%= proc.quantity%></td>
            <td><%= proc.processPrice%></td> 
            <td>
            <form method="POST" action="/<%= proc.id %>?_method=DELETE">
                <button type="submit">Удалить</button>
            </form>
            <a href="/process/<%= proc.id %>/edit">Изменить</a> </td> 
        </tr>
        
    <%});%>
</table>